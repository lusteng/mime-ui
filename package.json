{
  "name": "memui",
  "version": "0.1.3",
  "main": "./dist/index.js",
  "keywords": [
    "React Component",
    "UI"
  ],
  "scripts": {
    "dev": "react-scripts start",
    "build": "npm run rm && npm run build-ts && npm run  build-css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rm": "rimraf ./dist",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "test:nowatch": "cross-env CI=true react-scripts test --passWithNoTests",
    "prepublish": "npm run test:nowatch && npm run lint && npm run build",
    "site": "cross-env APP_ROOT=site dumi dev",
    "site-build": "cross-env APP_ROOT=site dumi build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^7.0.2",
    "dumi": "^1.0.31",
    "eslint": "^7.3.1",
    "husky": "^4.2.5",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
